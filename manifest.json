{
  "name": "JS Error Notification",
  "manifest_version": 2,
  "version": "0.1",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["/src/main.js"],
    "run_at": "document_end"
  }],
  "background" : {
    "scripts": ["src/notification.js"]
  },
  "web_accessible_resources" : [
    "/web_accessible_resources/index.js",
    "/web_accessible_resources/white-64x64.png"
  ],
  "permissions": [
    "notifications"
  ]
}
